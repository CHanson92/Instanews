{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "airbnb/hooks",
        "plugin:react/recommended",
        "react-hooks",
        "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error"],
        "react-hooks/rules-of-hooks": "error",
        "react-hooks/exhaustive-deps": "warn"
    }
}
